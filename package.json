{
	"name": "dosebot",
	"version": "0.0.1",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"dev": "concurrently -r 'npm:dev_a' 'npm:storybook'",
		"dev_a": "concurrently --hide 0 'npm:storybook-server'",
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --write .",
		"ts:check": "tsc --noEmit -p ./tsconfig.json",
		"build-dev": "eas build --profile development --platform android",
		"build-dev-local": "eas build --profile development --platform android --local --output dist/dev-build.apk",
		"build-local": "eas build --platform android --profile preview --local --output dist/build.apk",
		"build": "eas build --platform android --profile preview",
		"storybook-generate": "sb-rn-get-stories",
		"storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start",
		"storybook-server": "storybook dev -p 6006 -c ./.storybook-web",
		"test": "jest",
		"migrate": "drizzle-kit generate",
		"prebuild": "expo prebuild -p android"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.0.4",
		"@hookform/resolvers": "^4.1.0",
		"@notifee/react-native": "^9.1.8",
		"@pchmn/expo-material3-theme": "^1.3.2",
		"@react-native-masked-view/masked-view": "^0.3.2",
		"@react-navigation/drawer": "^7.1.1",
		"@react-navigation/native": "^7.0.14",
		"@react-navigation/native-stack": "^7.2.0",
		"@sentry/react-native": "6.3.0",
		"date-fns": "^4.1.0",
		"date-fns-jalali": "4.1.0-0",
		"drizzle-orm": "^0.39.3",
		"drizzle-zod": "^0.7.0",
		"expo": "~52.0.36",
		"expo-background-fetch": "~13.0.5",
		"expo-constants": "^17.0.7",
		"expo-crypto": "^14.0.2",
		"expo-dev-client": "~5.0.12",
		"expo-font": "^13.0.4",
		"expo-linking": "~7.0.5",
		"expo-localization": "^16.0.1",
		"expo-splash-screen": "~0.29.22",
		"expo-sqlite": "^15.1.2",
		"expo-status-bar": "~2.0.1",
		"expo-task-manager": "~12.0.5",
		"expo-updates": "^0.26.19",
		"i18next": "^24.2.2",
		"intl-pluralrules": "^2.0.1",
		"lodash": "^4.17.21",
		"optics-ts": "^2.4.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "^7.54.2",
		"react-i18next": "^15.4.1",
		"react-native": "0.76.7",
		"react-native-gesture-handler": "^2.20.2",
		"react-native-linear-gradient": "^2.8.3",
		"react-native-mmkv": "^3.2.0",
		"react-native-paper": "^5.13.1",
		"react-native-reanimated": "3.16.7",
		"react-native-safe-area-context": "^4.12.0",
		"react-native-screens": "^4.4.0",
		"react-native-vector-icons": "^10.2.0",
		"react-native-web": "~0.19.13",
		"superjson": "^2.2.2",
		"zod": "^3.24.2",
		"zod-i18n-map": "^2.27.0",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.26.9",
		"@biomejs/biome": "1.9.4",
		"@expo/metro-runtime": "^4.0.1",
		"@react-native-community/datetimepicker": "^8.2.0",
		"@react-native-community/slider": "^4.5.5",
		"@react-native/babel-preset": "^0.78.0",
		"@storybook/addon-essentials": "^8.5.8",
		"@storybook/addon-react-native-server": "^0.0.6",
		"@storybook/builder-vite": "^8.5.8",
		"@storybook/react": "^8.5.8",
		"@storybook/react-native": "^8.5.5",
		"@storybook/react-vite": "^8.5.8",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.15",
		"@types/react": "~18.3.18",
		"@types/react-native-vector-icons": "^6.4.18",
		"@vitejs/plugin-react-swc": "^3.8.0",
		"babel-loader": "^9.2.1",
		"babel-plugin-inline-import": "^3.0.0",
		"babel-plugin-react-native-web": "^0.19.13",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.30.4",
		"expo-drizzle-studio-plugin": "^0.1.1",
		"jest": "^29.7.0",
		"jest-expo": "^52.0.4",
		"lefthook": "^1.10.10",
		"react-native-web-linear-gradient": "^1.1.2",
		"storybook": "^8.5.8",
		"typescript": "^5.7.3",
		"vite-plugin-react-native-web": "^2.1.1"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
		]
	},
	"private": true,
	"trustedDependencies": ["@biomejs/biome", "@sentry/cli", "@swc/core"]
}
