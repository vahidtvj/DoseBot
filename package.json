{
	"name": "dosebot",
	"version": "0.0.1",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"dev": "concurrently -r 'npm:dev_a' 'npm:storybook'",
		"dev_a": "concurrently --hide 0 'npm:storybook-server'",
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --apply .",
		"ts:check": "tsc --noEmit -p ./tsconfig.json",
		"build-dev": "eas build --profile development --platform android",
		"build-dev-local": "eas build --profile development --platform android --local --output dist/dev-build.apk",
		"build-local": "eas build --platform android --profile preview --local --output dist/build.apk",
		"build": "eas build --platform android --profile preview",
		"storybook-generate": "sb-rn-get-stories",
		"storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
		"storybook-server": "storybook dev -p 6006 -c ./.storybook-web",
		"test": "jest"
	},
	"dependencies": {
		"@expo/vector-icons": "^13.0.0",
		"@hookform/resolvers": "^3.3.4",
		"@notifee/react-native": "^7.8.2",
		"@pchmn/expo-material3-theme": "^1.3.2",
		"@react-navigation/drawer": "^6.6.15",
		"@react-navigation/native": "^6.1.14",
		"@react-navigation/native-stack": "^6.9.22",
		"@sentry/react-native": "5.19.1",
		"date-fns": "^3.4.0",
		"expo": "~50.0.6",
		"expo-background-fetch": "~11.8.1",
		"expo-constants": "^15.4.5",
		"expo-crypto": "^12.8.1",
		"expo-dev-client": "~3.3.8",
		"expo-font": "^11.10.3",
		"expo-localization": "^14.8.3",
		"expo-status-bar": "~1.11.1",
		"expo-updates": "^0.24.11",
		"i18next": "^23.10.0",
		"intl-pluralrules": "^2.0.1",
		"jest": "^29.3.1",
		"jest-expo": "~50.0.3",
		"lodash": "^4.17.21",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.51.1",
		"react-i18next": "^14.0.5",
		"react-native": "0.73.2",
		"react-native-gesture-handler": "^2.16.0",
		"react-native-mmkv": "^2.12.1",
		"react-native-paper": "^5.12.3",
		"react-native-paper-dates": "^0.22.3",
		"react-native-reanimated": "^3.8.1",
		"react-native-safe-area-context": "^4.9.0",
		"react-native-screens": "^3.29.0",
		"react-native-vector-icons": "^10.0.3",
		"react-native-web": "~0.19.10",
		"superjson": "^2.2.1",
		"zod": "^3.22.4",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@biomejs/biome": "1.5.3",
		"@expo/metro-runtime": "^3.1.3",
		"@react-native-community/datetimepicker": "^7.6.2",
		"@react-native-community/slider": "^4.5.0",
		"@react-native/babel-preset": "^0.73.21",
		"@storybook/addon-essentials": "^7.6.17",
		"@storybook/addon-ondevice-actions": "^7.6.15",
		"@storybook/addon-ondevice-controls": "^7.6.15",
		"@storybook/addon-react-native-server": "^0.0.5",
		"@storybook/addon-react-native-web": "^0.0.23",
		"@storybook/react": "^7.6.17",
		"@storybook/react-native": "^7.6.15",
		"@storybook/react-webpack5": "^7.6.17",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.2.55",
		"@types/react-native-vector-icons": "^6.4.18",
		"babel-loader": "^8.3.0",
		"babel-plugin-react-native-web": "^0.19.10",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"lefthook": "^1.6.1",
		"storybook": "^7.6.17",
		"typescript": "^5.3.3"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
		]
	},
	"private": true
}
