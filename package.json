{
	"name": "dosebot",
	"version": "0.0.1",
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"dev": "concurrently -r 'npm:dev_a' 'npm:storybook'",
		"dev_a": "concurrently --hide 0 'npm:storybook-server'",
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --apply .",
		"ts:check": "tsc --noEmit -p ./tsconfig.json",
		"build-dev": "eas build --profile development --platform android",
		"build-dev-local": "eas build --profile development --platform android --local --output dist/dev-build.apk",
		"build-local": "eas build --platform android --profile preview --local --output dist/build.apk",
		"build": "eas build --platform android --profile preview",
		"storybook-generate": "sb-rn-get-stories",
		"storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
		"storybook-server": "storybook dev -p 6006 -c ./.storybook-web",
		"test": "jest",
		"migrate": "drizzle-kit generate"
	},
	"dependencies": {
		"@expo/vector-icons": "^14.0.2",
		"@hookform/resolvers": "^3.4.2",
		"@notifee/react-native": "^7.8.2",
		"@pchmn/expo-material3-theme": "^1.3.2",
		"@react-native-masked-view/masked-view": "^0.3.1",
		"@react-navigation/drawer": "^6.6.15",
		"@react-navigation/native": "^6.1.17",
		"@react-navigation/native-stack": "^6.9.26",
		"@sentry/react-native": "5.22.2",
		"date-fns": "^3.6.0",
		"date-fns-jalali": "3.6.0-0",
		"drizzle-orm": "^0.33.0",
		"drizzle-zod": "^0.5.1",
		"expo": "~51.0.11",
		"expo-background-fetch": "~12.0.1",
		"expo-constants": "^16.0.2",
		"expo-crypto": "^13.0.2",
		"expo-dev-client": "~4.0.18",
		"expo-font": "^12.0.7",
		"expo-localization": "^15.0.3",
		"expo-splash-screen": "~0.27.6",
		"expo-sqlite": "^14.0.6",
		"expo-status-bar": "~1.12.1",
		"expo-updates": "^0.25.17",
		"i18next": "^23.11.5",
		"intl-pluralrules": "^2.0.1",
		"jest": "^29.7.0",
		"jest-expo": "~51.0.2",
		"lodash": "^4.17.21",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.51.5",
		"react-i18next": "^14.1.2",
		"react-native": "0.74.2",
		"react-native-gesture-handler": "^2.16.2",
		"react-native-linear-gradient": "^2.8.3",
		"react-native-mmkv": "^2.12.2",
		"react-native-paper": "^5.12.3",
		"react-native-paper-dates": "^0.22.10",
		"react-native-reanimated": "^3.12.1",
		"react-native-safe-area-context": "^4.10.1",
		"react-native-screens": "^3.31.1",
		"react-native-vector-icons": "^10.1.0",
		"react-native-web": "~0.19.12",
		"superjson": "^2.2.1",
		"zod": "^3.23.8",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.24.6",
		"@biomejs/biome": "1.7.3",
		"@expo/metro-runtime": "^3.2.1",
		"@react-native-community/datetimepicker": "^8.0.1",
		"@react-native-community/slider": "^4.5.2",
		"@react-native/babel-preset": "^0.74.83",
		"@storybook/addon-essentials": "^7.6.17",
		"@storybook/addon-react-native-server": "^0.0.5",
		"@storybook/addon-react-native-web": "^0.0.23",
		"@storybook/react": "^7.6.17",
		"@storybook/react-native": "^7.6.15",
		"@storybook/react-webpack5": "^7.6.17",
		"@types/jest": "^29.5.12",
		"@types/react": "~18.2.79",
		"@types/react-native-vector-icons": "^6.4.18",
		"babel-loader": "^9.1.3",
		"babel-plugin-inline-import": "^3.0.0",
		"babel-plugin-react-native-web": "^0.19.12",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.24.2",
		"expo-drizzle-studio-plugin": "^0.0.2",
		"lefthook": "^1.6.12",
		"storybook": "^7.6.17",
		"typescript": "^5.3.3"
	},
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
		]
	},
	"private": true
}
